<Configuration>
    <ViewDefinitions>      
	<!-- ### DnsServerDirectoryPartition views ### -->
    	<View>
            <Name>DNSServerDirectoryPartition_TableView</Name>
            <ViewSelectedBy>
                <TypeName>Microsoft.Management.Infrastructure.CimInstance#DnsServerDirectoryPartition</TypeName>
            </ViewSelectedBy>
            <TableControl>
                <TableHeaders>
                    <TableColumnHeader>
                        <Label>DirectoryPartitionName</Label>
			<Width>45</Width>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>State</Label>
			<Width>29</Width>
                    </TableColumnHeader>                    
                    <TableColumnHeader>
                        <Label>Flags</Label>
			<Width>33</Width>
                    </TableColumnHeader>
                       <TableColumnHeader>
                        <Label>ZoneCount</Label>
			<Width>9</Width>
                    </TableColumnHeader>   
                </TableHeaders>
                <TableRowEntries>
                    <TableRowEntry>
                        <TableColumnItems>
                            <TableColumnItem>
                                <PropertyName>DirectoryPartitionName</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <ScriptBlock>
                                        if ($_.State -eq 0)
                                        {
                                            "0(DNS_DP_OKAY)"
                                        }
                                        elseif($_.State -eq 1)
                                        {
                                            "1(DNS_DP_STATE_REPL_INCOMING)"
                                        }
                                        elseif($_.State -eq 2)
                                        {
                                            "2(DNS_DP_STATE_REPL_OUTGOING)"
                                        }
                                        elseif($_.State -eq 3)
                                        {
                                            "3(DNS_DP_STATE_UNKNOWN)"
                                        }
                                        
                                </ScriptBlock>                                
                            </TableColumnItem>                            
                            <TableColumnItem>
                                <PropertyName>Flags</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>ZoneCount</PropertyName>
                            </TableColumnItem>
                         </TableColumnItems>
                    </TableRowEntry>
                </TableRowEntries>
            </TableControl>
        </View>	
        <View>
            <Name>DNSServerDirectoryPartition_ListView</Name>
