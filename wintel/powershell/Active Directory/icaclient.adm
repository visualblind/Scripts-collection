              NAME "<Disabled>"   VALUE "(none)"
              NAME "tab"   VALUE "tab"
              NAME "star"  VALUE "star"
              NAME "plus"  VALUE "plus"
              NAME "minus" VALUE "minus"
              NAME "F1"    VALUE "F1"
              NAME "F2"    VALUE "F2"
              NAME "F3"    VALUE "F3"
              NAME "F4"    VALUE "F4"
              NAME "F5"    VALUE "F5" DEFAULT
              NAME "F6"    VALUE "F6"
              NAME "F7"    VALUE "F7"
              NAME "F8"    VALUE "F8"
              NAME "F9"    VALUE "F9"
              NAME "F10"   VALUE "F10"
              NAME "F11"   VALUE "F11"
              NAME "F12"   VALUE "F12"
            END ITEMLIST
          END PART
        END POLICY
      ;
