{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "MSDN.windows-update",
          "folder_relative_path_in_docset": "./"
        }
      }
    },
    "fileMetadata": {},
    "template": [],
    "dest": "windows-update",
    "markdownEngineName": "markdig"
  }
}
