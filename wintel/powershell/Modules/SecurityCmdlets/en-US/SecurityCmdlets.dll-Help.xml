    <maml:navigationLink>
      <maml:linkText>Restore-AuditPolicy</maml:linkText>
    </maml:navigationLink>
    <maml:navigationLink>
      <maml:linkText>Backup-AuditPolicy</maml:linkText>
    </maml:navigationLink>
    <maml:navigationLink>
      <maml:linkText>Backup-SecurityPolicy</maml:linkText>
    </maml:navigationLink>
  </maml:relatedLinks>
</command:command>
