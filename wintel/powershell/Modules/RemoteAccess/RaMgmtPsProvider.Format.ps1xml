        <View>
            <Name>DAClientDnsConfiguration_Custom</Name>
            <ViewSelectedBy>
                <TypeName>Microsoft.Management.Infrastructure.CimInstance#DAClientDnsConfiguration</TypeName>
            </ViewSelectedBy>
            <CustomControl>
                <CustomEntries>
                    <CustomEntry>
                        <CustomItem>
                            <Frame>
                                <CustomItem>
                                    <ExpressionBinding>
                                        <ScriptBlock>
                                            $outputStr = "";
                                            if ($_.NrptGlobalSettings -ne $null) {
                                            $nrptGlobalProperties = @();
                                            $nrptGlobalStr   = "`r`n"
                                            $nrptGlobalStr  += "Local Name Resolution:`r`n";
                                            $nrptGlobalStr  += "======================`r`n";
                                            $_.NrptGlobalSettings.CimInstanceProperties | % { $nrptGlobalProperties += $_.Name }
                                            $nrptGlobalStr  += $_.NrptGlobalSettings | Format-List -Property $nrptGlobalProperties | Out-String
                                            $outputStr += $nrptGlobalStr;
                                            }

                                            if ($_.NrptEntry -ne $null) {
                                            $nrptEntryStr    = "Nrpt Rules:`r`n";
                                            $nrptEntryStr   += "===========`r`n";
                                            $nrptEntryProperties = @("Namespace", "DirectAccessDnsServers", "DirectAccessProxyName", "DirectAccessProxyType");
                                            if ($_.NrptEntry.Count -eq 1) {
                                            $nrptEntryStr += $_.NrptEntry[0] | Format-List -Property $nrptEntryProperties | Out-String
                                            }
                                            else {
                                            $nrptEntryStr += $_.NrptEntry | Format-Table -Property $nrptEntryProperties | Out-String
                                            }
                                            $outputStr += $nrptEntryStr;
                                            }
                                            $outputStr;
                                        </ScriptBlock>
                                    </ExpressionBinding>
                                </CustomItem>
                            </Frame>
                        </CustomItem>
                    </CustomEntry>
                </CustomEntries>
            </CustomControl>
        </View>
