        <View>
            <Name>ProtocolPreference_Custom</Name>
            <ViewSelectedBy>
                <TypeName>Microsoft.Management.Infrastructure.CimInstance#ProtocolPreference</TypeName>
            </ViewSelectedBy>
            <CustomControl>
                <CustomEntries>
                    <CustomEntry>
                        <CustomItem>
                            <Frame>
                                <CustomItem>
                                    <ExpressionBinding>
                                        <ScriptBlock>
                                            $outputStr = "";
                                            $numOfSpaces = 50;
                                            $outputStr += "Protocol                     ";
                                            $outputStr += "Priority (Lower value = higher priority) `r`n";
                                            $outputStr += "----------------------------------------------------------------------------`r`n";
                                            foreach($a in $_.ProtocolMetric)
                                            {
                                            $outputStr += $a.Name;
                                            $temp = $numOfSpaces - $a.Name.Length;
                                            while($temp -gt 0)
                                            {
                                            $outputStr += " ";
                                            $temp = $temp - 1;
                                            }
                                            $outputStr += $a.Level;
                                            $outputStr += "`r`n";
                                            }
                                            $outputStr
                                        </ScriptBlock>
                                    </ExpressionBinding>
                                </CustomItem>
                            </Frame>
                        </CustomItem>
                    </CustomEntry>
                </CustomEntries>
            </CustomControl>
        </View>
