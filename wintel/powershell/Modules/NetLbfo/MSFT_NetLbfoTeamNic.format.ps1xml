              <ListItem>
                <ItemSelectionCondition>
                  <ScriptBlock>$_.PSBase.CimInstanceProperties['ReceiveLinkSpeed'].Value -ge 1000000000</ScriptBlock>
                </ItemSelectionCondition>
                <PropertyName>ReceiveLinkSpeed(Gbps)</PropertyName>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>
