<?xml version="1.0" encoding="utf-8"?>

<!--  
  Composite Type Schema for Scenario: VolumeActivation  
  -->
<xs:schema
	targetNamespace="http://schemas.microsoft.com/mbca/models/VolumeActivation/2011/02"
